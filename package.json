{
  "name": "jsne-website",
  "version": "1.0.0",
  "private": true,
  "description": "jsne-website",
  "author": "nerdyman",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "bootstrap": "cp ./.env.example .env && yarn install --frozen-lockfile && husky install",
    "build": "gatsby build",
    "clean": "gatsby clean && rm -r src/generated",
    "develop": "gatsby develop",
    "lint": "eslint --rule \"'no-alert': 'error', 'no-console': 'error', 'no-debugger': 'error', '@typescript-eslint/no-unused-vars': ['error', { args: 'all', argsIgnorePattern: '^_', varsIgnorePattern: '^_', ignoreRestSiblings: true }]\"",
    "preload-fonts": "gatsby-preload-fonts",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "storybook": "start-storybook --ci -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 edge versions",
      "last 2 firefox versions",
      "last 2 safari versions",
      "not dead",
      "not ie > 0",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pretty-quick --staged",
      "yarn lint -- --cache --fix",
      "yarn test --bail --findRelatedTests"
    ],
    "**/*.{css}": [
      "pretty-quick --staged"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "importOrder": [
      "^~/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "printWidth": 90,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@stitches/core": "0.2.2",
    "@stitches/react": "0.2.2",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "dotenv": "^8.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "gatsby": "^3.0.3",
    "gatsby-image": "^3.0.0",
    "gatsby-plugin-image": "^1.3.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-mdx": "^2.9.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-preload-fonts": "^2.0.0",
    "gatsby-plugin-react-helmet-async": "^1.1.1",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^3.0.0",
    "gatsby-source-contentful": "^5.3.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "modern-normalize": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-polymorphic-box": "^3.0.3",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-a11y": "^6.3.4",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-storysource": "^6.3.4",
    "@storybook/addon-viewport": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.1.0",
    "@trivago/prettier-plugin-sort-imports": "^1.4.4",
    "@types/jest": "^26.0.20",
    "@types/mdx-js__react": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-loader": "^8.2.2",
    "contentful-cli": "^1.5.35",
    "eslint": "^7.30.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^5.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
