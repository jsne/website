{
  "name": "jsne-website",
  "version": "1.0.0",
  "private": true,
  "description": "jsne-website",
  "author": "nerdyman",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "bootstrap": "cp ./env.example .env && yarn install --frozen-lockfile",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "lint": "eslint --rule \"'no-alert': 'error', 'no-console': 'error', 'no-debugger': 'error', '@typescript-eslint/no-unused-vars': ['error', { args: 'all', argsIgnorePattern: '^_', varsIgnorePattern: '^_', ignoreRestSiblings: true }]\"",
    "preload-fonts": "gatsby-preload-fonts",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "storybook": "start-storybook --ci -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 edge versions",
      "last 2 firefox versions",
      "last 2 safari versions",
      "not dead",
      "not ie > 0",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pretty-quick --staged",
      "yarn lint -- --cache --fix",
      "yarn test --bail --findRelatedTests"
    ],
    "**/*.{css}": [
      "pretty-quick --staged"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 90,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "resolutions": {
    "graphql": "^14.6.0"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@stitches/core": "^0.0.3-canary.4",
    "@stitches/react": "^0.0.3-canary.4",
    "@svgr/webpack": "^5.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "dotenv": "^8.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-mdx": "^1.8.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-preload-fonts": "^1.7.0",
    "gatsby-plugin-react-helmet-async": "^1.1.0",
    "gatsby-plugin-sharp": "^2.12.0",
    "gatsby-plugin-sitemap": "^2.10.0",
    "gatsby-plugin-svgr": "^2.1.0",
    "gatsby-plugin-typegen": "^2.2.2",
    "gatsby-plugin-typescript": "^2.10.0",
    "gatsby-source-contentful": "^4.4.0",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-transformer-sharp": "^2.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.7",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-a11y": "^6.2.0-alpha.15",
    "@storybook/addon-actions": "^6.2.0-alpha.15",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-essentials": "^6.2.0-alpha.15",
    "@storybook/addon-links": "^6.2.0-alpha.15",
    "@storybook/addon-storysource": "^6.2.0-alpha.15",
    "@storybook/addon-viewport": "^6.2.0-alpha.15",
    "@storybook/react": "^6.2.0-alpha.15",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^4.0.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.2.2",
    "contentful-cli": "^1.5.10",
    "eslint": "^7.17.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
